                                    <li><a href="checkout.html">Delivery information</a></li>
                                </ul>
                            </div>
                            <div class="top_left">
                                @Html.Partial("_LoginPartial")
                                <h2><span></span> Call us : 032 2352 782</h2>
                            </div>
                            <div class="clearfix"> </div>
                        </div>

                    </div>
                    <div class="clearfix"></div>
                    <!-- /top_bg -->
                </div>
                <div class="header_bg">

                    <div class="header">
